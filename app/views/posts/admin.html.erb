<div class="container well">
<% @posts.each do |post| %>
<h2><%= link_to post.title, post %></h2>
<p><%= post.tag_list%>
<p><%= post.subtitle%></p>
<p><%= post.content %></p>
<p><%= post.img%>
<p><%= link_to "Edit", edit_post_path(post)%>
<%= link_to "Delete", post, :method => :delete %></p>
<hr />
<% end %>
</div>